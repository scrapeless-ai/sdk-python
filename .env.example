# Scrapeless API Credentials
SCRAPELESS_API_KEY=your_api_key_here
SCRAPELESS_BASE_API_URL=https://api.scrapeless.com
SCRAPELESS_ACTOR_API_URL=https://actor.scrapeless.com
SCRAPELESS_STORAGE_API_URL=https://storage.scrapeless.com
SCRAPELESS_BROWSER_API_URL=https://browser.scrapeless.com
SCRAPELESS_CRAWL_API_URL=https://api.scrapeless.com

# Actor Related Environment Variables
SCRAPELESS_ACTOR_ID=your_actor_id
SCRAPELESS_RUN_ID=your_run_id
SCRAPELESS_USER_ID=your_user_id
SCRAPELESS_TEAM_ID=your_team_id
SCRAPELESS_INPUT=your_input_data

# Storage IDs
SCRAPELESS_KV_NAMESPACE_ID=your_kv_namespace_id
SCRAPELESS_DATASET_ID=your_dataset_id
SCRAPELESS_BUCKET_ID=your_bucket_id
SCRAPELESS_QUEUE_ID=your_queue_id

# Logging Configuration
SCRAPELESS_LOG_MAX_SIZE=100m
SCRAPELESS_LOG_MAX_BACKUPS=5
SCRAPELESS_LOG_MAX_AGE=7
SCRAPELESS_LOG_ROOT_DIR=./logs
SCRAPELESS_LOG_ENABLED=true